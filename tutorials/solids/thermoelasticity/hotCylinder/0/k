FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -3 -1 0 0 0];

internalField   uniform 250;

boundaryField
{
    left
    {
        type            symmetryPlane;
    }
    bottom
    {
        type            symmetryPlane;
    }

    inside
    {
        type            zeroGradient;
    }
    outside
    {
        type            zeroGradient;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
}

