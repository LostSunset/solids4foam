FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      solidProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// linearGeometry: assumes small strains and rotations
solidModel     explicitLinearGeometryTotalDisplacement;

explicitLinearGeometryTotalDisplacementCoeffs
{
    // Numerical viscosity for stabilisation
    numericalViscosity    eta [ 0 0 -1 0 0 0 0 ] 1000;

    // Maximum number of momentum correctors
    nCorrectors     0;

    // Write frequency for the residuals
    infoFrequency   0;
}


