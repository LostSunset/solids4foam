FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      topoSetDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

actions
(
    {
        name    outer;
        type    cellZoneSet;
        action  new;
        source cylinderToCell;
        sourceInfo
        {
            p1       (0.0 0.0 -100); // start point on cylinder axis
            p2       (0.0 0.0  100);   // end point on cylinder axis
            radius   100e-3;
        }
    }
    {
        name    inner;
        type    cellZoneSet;
        action  new;
        source cylinderToCell;
        sourceInfo
        {
            p1       (0.0 0.0 -100); // start point on cylinder axis
            p2       (0.0 0.0  100);   // end point on cylinder axis
            radius   70e-3;
        }
    }
    {
        name 	outer;
        type    cellZoneSet;
        action	delete;
        source  zoneToCell;
        sourceInfo
        {
            zone inner;
        }
			
    }
);

