FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      sigmaHyd;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    bottom
    {
        type            empty;
    }
    top
    {
        type            empty;
    }
    inner-wall
    {
        type            extrapolatedPressureValue;
        relaxFactor		0.1;
        value			uniform 0;
    }
    outer-wall
    {
        type            extrapolatedPressureValue;
        relaxFactor		0.1;
        value			uniform 0;
    }
    symmetry-x
    {
        type            solidSymmetry;
        patchType       symmetryPlane;
        value			uniform 0;
    }
    symmetry-y
    {
        type            solidSymmetry;
        patchType       symmetryPlane;
        value			uniform 0;
    }
}

// ************************************************************************* //
