FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      mechanicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

planeStress     no;

mechanical
(
    rubber
    {
        type            GentElastic;
        rho             rho [1 -3 0 0 0 0 0] 1100;
        E               E [1 -1 -2 0 0 0 0] 6e+6;
        nu              nu [0 0 0 0 0 0 0] 0.4;
        temperature	temperature [0 0 0 1 0 0 0] 305;
        ilim		ilim [0 0 0 0 0 0 0] 7.04;
        N 		N [0 0 0 0 0 0 0] 0.0158;
        Vs 		Vs [0 3 0 0 0 0 0] 1.81e-5;
    }
);


