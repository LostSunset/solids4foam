FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      DD;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    round
    {
        type            solidTraction;
        traction        uniform ( 0 0 0 );
        pressure        uniform 0;
        value           uniform (0 0 0);
    }
    up
    {
        type            fixedDisplacement;
        value           uniform (0 0 0);
    }
    symm
    {
        type            symmetryPlane;
    }
    down
    {
        type            solidTraction;
        traction        uniform ( 0 0 -1.737e+06 );
        pressure        uniform 0;
        value           uniform (0 0 0);
    }

}

// ************************************************************************* //
