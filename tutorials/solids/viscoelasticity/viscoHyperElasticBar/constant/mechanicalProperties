FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      mechanicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

planeStress     no;

mechanical
(
    rubber
    {
        type            viscoNeoHookeanElastic;
        rho             rho [1 -3 0 0 0 0 0] 1000;
        EInfinity       E [1 -1 -2 0 0 0 0] 2e+09;
        E               ( 2e+09 2e+09);
        k               k  [1 -1 -2 0 0 0 0] 2.0e+09;
        mu0             mu0 [1 -1 -2 0 0 0 0] 6.30e+05;     // Ogden model
        mu1             mu1 [1 -1 -2 0 0 0 0] 0.012e+05;
        mu2             mu2 [1 -1 -2 0 0 0 0] -0.10e+05;
        alpha           (1.3 5.0 -2.0);
        beta		beta [0 0 0 0 0 0 0] 9.0;          // empirical coefficient, (G. A. HOLZAPFEL,1996)
        relaxationTimes ( 30 20);
        //nu              nu [0 0 0 0 0 0 0] 0.5;
        nu              nu [0 0 0 0 0 0 0] 0.3;
    }
);

