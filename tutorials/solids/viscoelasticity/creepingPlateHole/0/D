FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      D;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    left
    {
        type            symmetryPlane;
        skewCorrected   yes;
    }
    right
    {
        // This is an example of how you can set a pressure field based on a
        // point cloud
        type            solidTraction;
        traction        uniform (0 0 0);
        pressureField   "pressureOnRight";
        value           uniform (0 0 0);
    }
    down
    {
        type            symmetryPlane;
        skewCorrected   yes;
    }
    up
    {
        // This is an example of how you can set a traction field based on a
        // point cloud
        type            solidTraction;
        tractionField   "tractionOnUp";
        pressure        uniform 0;
        value           uniform (0 0 0);
    }
    hole
    {
        type            solidTraction;
        traction        uniform ( 0 0 0 );
        pressure        uniform 0;
        value           uniform (0 0 0);
    }
    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
