FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

d2dt2Schemes
{
    default            steadyState;
}

ddtSchemes
{
    default            steadyState;
}

gradSchemes
{
    default            none;
    grad(D)            extendedLeastSquares 0;
    grad(p)            leastSquares;
    snGradCorr(D)      extendedLeastSquares 0;
    snGradCorr(p)      extendedLeastSquares 0;
}

divSchemes
{
    default            none;
    div(sigma)         Gauss linear;
    div(D)             Gauss linear;
    skewCorrectedDiv(D) Gauss skewCorrected linear;
}

laplacianSchemes
{
    default            none;
    laplacian(DD,D)    Gauss linear corrected;
    laplacian(DDD,DD)  Gauss linear corrected;
    laplacian(Dp,p)    Gauss linear corrected;
    laplacian(rUAf,p)  Gauss linear corrected;
}

snGradSchemes
{
    default            none;
    snGrad(D)          corrected;
    snGrad(DD)         corrected;
}

interpolationSchemes
{
    default            none;
    interpolate(impK)  linear;
    interpolate(grad(D)) linear;
    interpolate((1|A(D))) linear;
    interpolate(grad(p)) linear;
}

// ************************************************************************* //
