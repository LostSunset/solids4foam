FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

d2dt2Schemes
{
    default            none;
    d2dt2(D)           steadyState;
    d2dt2(DD)          steadyState;
}

ddtSchemes
{
    default            none;
    ddt(D)             steadyState;
    ddt(DD)            steadyState;
}

gradSchemes
{
    default            none;
    grad(D)            extendedLeastSquares 0;
    grad(DD)           extendedLeastSquares 0;
    snGradCorr(D)      extendedLeastSquares 0;
    snGradCorr(DD)     extendedLeastSquares 0;
}

divSchemes
{
    default            none;
    div(sigma)         Gauss linear;
}

laplacianSchemes
{
    default            none;
    laplacian(DD,D)    Gauss linear corrected;
    laplacian(DDD,DD)  Gauss linear corrected;
}

snGradSchemes
{
    default            none;
    snGrad(D)          corrected;
    snGrad(DD)         corrected;
}

interpolationSchemes
{
    default            none;
    interpolate(impK)  harmonic;
    interpolate(grad(D)) linear;
    interpolate(grad(DD)) linear;
}

// ************************************************************************* //
