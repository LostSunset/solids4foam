FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     solidFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         7000;

deltaT          700;

writeControl    timeStep;

writeInterval   1;

cycleWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable no;

functions
(
    pointStressSymmy
    {
        type    solidPointStress;
        point   (0.0075 0 0);
    }
    pointStressSymmx
    {
        type    solidPointStress;
        point   (0 0.0075 0);
    }
    pointDisp
    {
        type    solidPointDisplacement;
        point   (0.0075 0 0);
    }
);

