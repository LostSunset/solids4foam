FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    left
    {
        type            solidSymmetry;
        patchType       symmetryPlane;
        value           uniform 0;
    }
    right
    {
        //type            fixedValue;
        type            extrapolated;
        value           uniform 0;
        //value           $internalField;
        //value           uniform 0;
    }
    down
    {
        type            solidSymmetry;
        patchType       symmetryPlane;
        value           uniform 0;
    }
    up
    {
        type            zeroGradient;
    }
    hole
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}

