FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

d2dt2Schemes
{
    default none;
    ddt(pointU)  Euler;
}

ddtSchemes
{
    default none;
    ddt(pointU)  Euler;
}

gradSchemes
{
    default leastSquares;
}

divSchemes
{
    default none;
}

laplacianSchemes
{
    default none;
}

interpolationSchemes
{
    default linear;
}

snGradSchemes
{
    default none;
}

